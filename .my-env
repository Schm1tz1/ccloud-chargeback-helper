CCLOUD_BILLING_API_KEY=<Your_API_Key>                                   #e.g. 1234567890
# CCLOUD_BILLING_API_KEY=1234567890

CCLOUD_BILLING_API_SECRET=<YOUR_API_SECRET>                             #e.g. asdfghjklpoiuytrewq1234567890
#  CCLOUD_BILLING_API_SECRET=asdfghjklpoiuytrewq1234567890

METRICS_API_SERVER_URL=<Your_Metrics_API_Prometheus_Server>             #e.g. http://localhost:9090
#  METRICS_API_SERVER_URL=http://localhost:9090

METRICS_API_SERVER_ENABLE_AUTH=<True/False>                             #True if your metrics server is protected by basic auth else False
# METRICS_API_SERVER_ENABLE_AUTH=True

METRICS_API_SERVER_AUTH_TYPE=HTTPBasicAuth                              #HTTPBasic is the only supported auth type as of now. Do NOT change this

METRICS_API_SERVER_AUTH_USERNAME=<Your_Metrics_API_Prom_Server_User>    #e.g. admin
# METRICS_API_SERVER_AUTH_USERNAME=admin

METRICS_API_SERVER_AUTH_PASSWORD=<Your_Metrics_API_Prom_Server_Pass>    #e.g. password
# METRICS_API_SERVER_AUTH_PASSWORD=password

CHARGEBACK_SERVER_URL=http://prometheus_for_chargeback:9090             #Do NOT change this if you will be using the provided docker-compose file
